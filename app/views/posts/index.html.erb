<h1>Posts</h1>
<table class="table">
    <tr>
        <th>Titulo</th>
        <th>Fecha ingreso</th>
        <th>Imagen</th>
        <th>Contenido</th>
    </tr>
    <% @posts.present? && @posts.each do |post|%>
        <tr>
            <td><%= post.title%></td>
            <td><%= post.created_at%></td>
            <td><%=image_tag post.image_url%></td>
            <td><%= post.content%></td>
        </tr>
        <% end %>
    </table>
